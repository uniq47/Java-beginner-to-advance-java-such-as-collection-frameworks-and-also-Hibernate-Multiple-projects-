package practice;

public class Flowcontrol10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		/*
		 * Write a program to print numbers from 1 to 10 in a single row with one tab
		 * space. For Loop
		 */
		
		for(int i = 0; i<10; i++) {
			System.out.println(i);
		}
	}

}
