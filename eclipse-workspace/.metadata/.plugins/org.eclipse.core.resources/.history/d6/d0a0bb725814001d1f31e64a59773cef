package seocnd_case;

public abstract class Employee {

	private int eno;
	private String ename;
	private String dname;
	public int getEno() {
		return eno;
	}
	public void setEno(int eno) {
		this.eno = eno;
	}
	public String getEname() {
		return ename;
	}
	public void setEname(String ename) {
		this.ename = ename;
	}
	public String getDname() {
		return dname;
	}
	public void setDname(Stirng dname) {
		this.dname = dname;
	}
	
public abstract double CalcualteSalary(String dname);
}
