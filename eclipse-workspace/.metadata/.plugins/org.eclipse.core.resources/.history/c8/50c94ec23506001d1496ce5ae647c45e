package day4;

public class RelationEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//relational operator : >  , < , >=, <=
		
		int a = 10;
		int b = 20;
		
		System.out.println(a>b);
		
	//	equality operators
		a = 20;
		System.out.println(a ==b); 
	}
	

	
	
}
