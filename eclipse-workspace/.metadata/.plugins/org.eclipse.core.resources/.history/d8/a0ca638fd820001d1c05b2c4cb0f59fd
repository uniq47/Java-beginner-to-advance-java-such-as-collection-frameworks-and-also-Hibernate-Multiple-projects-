 import java.util.HashMap;
 
 public class HashMapDemo
 {
	 public static void main(String [] args)
	 {
		 HashMap<Integer,String> hm = new HashMap<>();
		 hm.put(47, "Uniq");
		 hm.put(21, "aayam");
		 hm.put(1, "Shanti");
		 hm.put(2, "Upendra");
		 String name = hm.get(47);
		 System.out.println(hm);
		 System.out.println(name);
	 }
 }