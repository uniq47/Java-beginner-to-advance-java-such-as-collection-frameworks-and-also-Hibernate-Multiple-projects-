package day9;

public class SwitchEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		byte b = 50;
		
		switch(b)
		{
			case 10:
			System.out.println("Hello bro");
			
			case 70:
			System.out.println("yo");
	
			default:
			System.out.println("Damn");
		}
		
		
		
		
		
	}

}
