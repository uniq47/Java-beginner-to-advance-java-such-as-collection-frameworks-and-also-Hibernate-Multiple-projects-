package java5;

import java.util.ArrayList;

public class VaragsEx {
	
	static void  display(String ... values)
	{
		for(String s : values )
		{
			System.out.println(s);
		}
		System.out.println("... Display  Method Invoked ... ");
	}
	
	
	static void  display1(ArrayList ... values)
	{
		for(ArrayList s : values )
		{
			System.out.println(s);
		}
		System.out.println("... Display  Method Invoked ... ");
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		display();
		System.out.println("==================================================");
		display("yooo", "What's up from uniq", "lets have fun");
		System.out.println("==================================================");
		display("Whats the plan");
		System.out.println("==================================================");
		display("Seee yahhh"); 
		System.out.println("==================================================");
		display1();
		System.out.println("==================================================");
	//	display1("suprise"); error will come so add like this 
		//display1(new ArrayList().add("uniq"));
		ArrayList al = new ArrayList();
				al.add("uniq");
		display1(al);
		
		
	}

}
